# GitHub App ID (find on your GitHub App settings page)
APP_ID=your_github_app_id

# The org or username where your GitHub App is installed
OWNER=your_github_org_or_username

# How many days old before workflow runs are deleted (default: 30)
DAYS_OLD=30

# Base64-encoded contents of your .pem private key (never commit the actual key)
PRIVATE_KEY_BASE64=base64_encoded_private_key_here

# Enable dry run mode (set to 'true' to only log, not delete)
DRY_RUN=true
